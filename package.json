{
    "name": "react-boilerplate",
    "version": "0.0.2",
    "main": "index.js",
    "license": "MIT",
    "browserslist": [
        "> 1%",
        "ie > 9"
    ],
    "scripts": {
        "start": "webpack-dev-server --open --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js",
        "test": "jest",
        "serve": "serve build"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>/src/utils/testConfig/test-config.js",
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/utils/testConfig/assetsTransformer.js",
            "\\.(css|less)$": "<rootDir>/src/utils/testConfig/assetsTransformer.js"
        }
    },
    "devDependencies": {
        "autoprefixer": "^7.1.5",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-module-resolver": "^2.7.1",
        "babel-preset-env": "^1.6.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "clean-webpack-plugin": "^0.1.17",
        "css-loader": "^0.28.7",
        "enzyme": "^3.1.0",
        "enzyme-adapter-react-16": "^1.0.3",
        "file-loader": "^1.1.5",
        "html-webpack-plugin": "^2.30.1",
        "jest": "^21.2.1",
        "json-loader": "^0.5.7",
        "postcss-loader": "^2.0.8",
        "react-hot-loader": "^3.1.1",
        "react-test-renderer": "^16.0.0",
        "serve": "^6.4.0",
        "style-loader": "^0.19.0",
        "uglifyjs-webpack-plugin": "^1.1.4",
        "url-loader": "^0.6.2",
        "webpack": "^3.5.6",
        "webpack-dev-server": "^2.7.1",
        "webpack-merge": "^4.1.1"
    },
    "dependencies": {
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    }
}
